{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "./.astro/types",
      "astro/client",
      "vite-plugin-pwa/client",
      "vite-plugin-pwa/info",
      "vite-plugin-pwa"
    ],
    "baseUrl": ".",
    "paths": {
      "@*": ["./src/*"]
    }
  }
}
