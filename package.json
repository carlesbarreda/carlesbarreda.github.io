{
  "name": "astro-paper",
  "version": "2.1.0",
  "private": false,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "check": "astro check && prettier --check . && eslint . && tsc --jsx preserve --noEmit",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "cz": "cz",
    "prepare": "husky install",
    "lint:check": "eslint .",
    "lint": "eslint --fix ."
  },
  "dependencies": {
    "@astrojs/rss": "^2.1.0",
    "astro": "^2.0.8",
    "fuse.js": "^6.6.2",
    "github-slugger": "^2.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^8.0.1",
    "satori": "^0.2.3",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@astrojs/react": "^2.0.2",
    "@astrojs/sitemap": "^1.0.1",
    "@astrojs/tailwind": "^3.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vite-pwa/astro": "^0.0.1",
    "astro-eslint-parser": "^0.11.0",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.33.0",
    "eslint-plugin-astro": "^0.23.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-pwa": "^0.14.4",
    "workbox-window": "^6.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --plugin-search-dir=. --write"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "tailwindcss/nesting": {},
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
