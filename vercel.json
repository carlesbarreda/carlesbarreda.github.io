{
  "framework": "astro",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev --site https://log.carlesbarreda.cat --port $PORT",
  "buildCommand": "pnpm build --site https://log.carlesbarreda.cat",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        }
      ]
    }
  ]
}
