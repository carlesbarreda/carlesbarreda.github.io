{
  "framework": "astro",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "devCommand": "pnpm dev --site https://carlesbarreda-github-io.vercel.app --base /astro-paper --port $PORT",
  "buildCommand": "pnpm build --site https://carlesbarreda-github-io.vercel.app --base /astro-paper",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        }
      ]
    }
  ]
}
