{
  "framework": "astro",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "devCommand": "pnpm dev --site https://carlesbarreda-github-io.vercel.app --port $PORT",
  "buildCommand": "pnpm build -- astro_args=\"--site https://carlesbarreda-github-io.vercel.app\"",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        }
      ]
    }
  ]
}
